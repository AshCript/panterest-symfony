{% extends 'base.html.twig' %}

{% block title 'Editing pin ' ~ pin.id %}

{% block body %}
    <h1>Editing Pin {{ pin.id }}</h1>
    {{ form_start(pinForm) }}
        {{ form_widget(pinForm) }}
        <input type='submit' value='Save'/>
    {{ form_end(pinForm) }}

    <a href='{{ path('app_home') }}'>Go back</a>
{% endblock %}
